{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[less]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.quickSuggestions": {
      "comments": "on",
      "other": "on",
      "strings": "on"
    }
  },
  "[nginx]": {
    "editor.defaultFormatter": "teclado.vscode-nginx-format"
  },
  "[postcss]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "cSpell.userWords": [
    "antd",
    "asar",
    "audiobuffer",
    "chatgpt",
    "choiced",
    "coturn",
    "echarts",
    "esbuild",
    "gitee",
    "iife",
    "juejin",
    "keypoints",
    "mobilenet",
    "openai",
    "outfile",
    "pinia",
    "posenet",
    "preinstall",
    "QIANKUN",
    "qrcode",
    "Rong",
    "Sider",
    "tailwindcss",
    "Tesseract",
    "tfjs",
    "unocss",
    "wallhaven",
    "wangrongding",
    "webgl",
    "wechat",
    "Wechaty",
    "wireframe"
  ],
  "cSpell.flagWords": ["hte", "destory", "conts"],
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.maxColumn": 50,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~@!#%^&*()=+[{]}\\|;:'\",.<>?Ôºå„ÄÇ„ÄÅ„Ää„ÄãÔºüÔºõÔºö„Äê„Äë‚Äú‚Äù",
  "editor.wordWrapColumn": 200,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "wxml": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "eslint.packageManager": "yarn",
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "files.associations": {
    "*.cjson": "jsonc",
    "*.json": "jsonc",
    "*.wxs": "javascript",
    "*.wxss": "css"
  },
  "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "git.autofetch": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": true,
    "typescript": true,
    "vue": true,
    "html": true
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.avatars": false,
  "gitlens.hovers.currentLine.details": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "leetcode.defaultLanguage": "javascript",
  "leetcode.endpoint": "leetcode-cn",
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.workspaceFolder": "e:\\Noah-Programs\\leetcut",
  "markdown-preview-enhanced.previewTheme": "newsprint.css",
  "markdown.experimental.editor.pasteLinks.enabled": true,
  "markdown.experimental.updateLinksOnFileMove.enabled": "prompt",
  "markdown.experimental.validate.fileLinks.enabled": "error",
  "markdown.preview.fontSize": 16,
  "minapp-vscode.disableAutoConfig": true,
  "powermode.enabled": true,
  "powermode.explosions.gifMode": "restart",
  "powermode.explosions.offset": 0.4,
  "powermode.presets": "fireworks",
  "powermode.shake.enabled": false,
  "powermode.shake.intensity": 0,
  "quokka.startViewStatusBar": true,
  "redhat.telemetry.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  "stylelint.packageManager": "yarn",
  "stylelint.snippet": ["css", "less", "postcss", "scss", "sass"],
  "stylelint.validate": ["css", "less", "postcss", "scss", "sass"],
  "tailwindCSS.emmetCompletions": true,
  "todo-tree.highlights.customHighlight": {
    "FIXME": {
      "foreground": "black",
      "gutterIcon": true,
      "iconColour": "yellow"
    },
    "TODO": {
      "background": "#e00",
      "fontWeight": "bold",
      "foreground": "white",
      "icon": "heart-fill",
      "iconColour": "green",
      "type": "line"
    }
  },
  "todo-tree.highlights.defaultHighlight": {
    "background": "white",
    "foreground": "red",
    "icon": "alert",
    "iconColour": "blue",
    "opacity": 50,
    "type": "text"
  },
  "turboConsoleLog.addSemicolonInTheEnd": true,
  "turboConsoleLog.delimiterInsideMessage": "/",
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "turboConsoleLog.insertEnclosingClass": false,
  "turboConsoleLog.insertEnclosingFunction": false,
  "turboConsoleLog.logMessagePrefix": "üöÄüöÄüöÄ",
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "versionlens.suggestions.showOnStartup": true,
  "volar.codeLens.scriptSetupTools": true,
  "vsicons.dontShowNewVersionMessage": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorTheme": "One Dark Pro",
  "workbench.editor.enablePreview": false,
  "workbench.experimental.settingsProfiles.enabled": true,
  "workbench.iconTheme": "vscode-icons",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "svg.preview.mode": "svg",
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "window.zoomLevel": 1,
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  }
}
