{
  "private": "true",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "changeset:init": "npx changeset init",
    "changeset:add": "changeset add",
    "clean-install": "pnpm -r exec rm -rf node_modules && pnpm i",
    "pre:dev": "wait-on ./packages/tiny-player/dist/index.js",
    "dev": "concurrently \"pnpm dev:player\" \"pnpm run pre:dev && pnpm dev:playground\"",
    "dev:parallel": "pnpm -r --no-sort dev",
    "--- player --------------------------------------------": "",
    "dev:player": "pnpm -F \"tiny-player\" dev",
    "build": "pnpm -F \"tiny-player\" build",
    "--- playground ----------------------------------------": "",
    "dev:playground": "pnpm -F playground dev",
    "build:playground": "pnpm -F playground build"
  },
  "author": "荣顶",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "concurrently": "^8.0.1",
    "wait-on": "^7.0.1"
  }
}
